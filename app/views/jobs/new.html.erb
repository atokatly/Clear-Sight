<%= form_for @job do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :job_address %>
  <%= f.text_field :address %>
  <%= f.label :company_field, placeholder: "Glass, Contruction" %>
  <%= f.text_field :company_field %>
  <%= f.hidden_field :user_id, value: @user.id %>
  <%= f.submit "Create Job" %>
<%end%>
