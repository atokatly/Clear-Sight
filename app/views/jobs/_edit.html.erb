<%= form_for @job do |f| %>
  <%=f.label :title %>
  <%=f.text_field :title %>
  <%=f.label :description %>
  <%=f.text_field :description %>
  <%=f.label :company_field %>
  <%=f.text_field :company_field %>
  <%=f.label :job_address %>
  <%=f.text_field :address %>
  <%=f.submit "Update Job" %>
<%end%>
